{
    "kind": "collectionType",
    "collectionName": "users",
    "info": {
        "singularName": "user",
        "pluralName": "users",
        "displayName": "User",
        "description": ""
    },
    "options": {
        "draftAndPublish": false
    },
    "attributes": {
        "username": {
            "type": "string",
            "required": true
        },
        "email": {
            "type": "email",
            "required": true
        },
        "password": {
            "type": "password",
            "required": true
        },
        "bio": {
            "type": "text"
        },
        "profile_image": {
            "type": "media",
            "multiple": false,
            "required": false
        },
        "user_type": {
            "type": "string"
        },
        "following": {
            "type": "json"
        },
        "followers": {
            "type": "json"
        },
        "is_premium": {
            "type": "boolean",
            "default": false
        },
        "is_deleted": {
            "type": "boolean",
            "default": false
        },
        "bookmarks": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::bookmark.bookmark",
            "mappedBy": "user"
        }
    }
}
